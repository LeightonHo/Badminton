{
  "homepage": "./",
  "name": "crosscourt",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@material-ui/core": "^4.12.3",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.19",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.1",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "axios": "^1.6.2",
    "env-cmd": "^10.1.0",
    "moment": "^2.29.4",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-confirm-alert": "^3.0.6",
    "react-dom": "^18.2.0",
    "react-facebook-login": "^4.1.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.20.0",
    "react-scripts": "^5.0.1",
    "typescript": "^5.3.2",
    "web-vitals": "^3.5.0"
  },
  "scripts": {
    "git-info": "node src/gitInfo.js",
    "start": "npm run git-info && HTTPS=true && react-scripts start",
    "build:test": "npm run git-info && env-cmd -f .env react-scripts build",
    "build:production": "npm run git-info &&  env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy:test": "npm run build:test && aws s3 sync build/ s3://test-app.crosscourt.net --acl public-read && aws cloudfront create-invalidation --distribution-id EJ9OQ2EW38C8Q --paths \"/*\"",
    "deploy:production": "npm run build:production && aws s3 sync build/ s3://app.crosscourt.net --acl public-read && aws cloudfront create-invalidation --distribution-id E2BLDZ09GAFWAR --paths \"/*\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/jsx-uses-react": "off",
      "react/prop-types": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-facebook-login": "^4.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint-plugin-react": "^7.33.2"
  }
}
